[sub_resource type="GDScript" id="GDScript_npqj3"]
script/source = "extends PanelContainer

@onready
var grid := $GridContainer

@onready
var icons := {
	\"forward\": $GridContainer/ActionIcon2,
	\"left\": $GridContainer/ActionIcon4,
	\"backward\": $GridContainer/ActionIcon5,
	\"right\": $GridContainer/ActionIcon6,
}

func _input(event):
	var any_pressed := false
	for action in icons.keys():
		if Input.is_action_pressed(action):
			icons[action].modulate.a = 1.
			any_pressed = true
		else:
			icons[action].modulate.a = .2
"

[sub_resource type="GDScript" id="GDScript_ld75c"]
script/source = "extends Label

@export
var property := \"\"

var player : Player :
	get:
		return Globals.player

func _process(delta):
	if property != \"\" and player:
		var val = player.get(property)
		if val:
			if val is Vector2:
				val = snapped(val, Vector2.ONE * .001)
			if val is Vector3:
				val = snapped(val, Vector3.ONE * .001)
				
			text = str(val)
"

