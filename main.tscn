script/source = "extends Node3D

func _ready():
	Input.mouse_mode = Input.MOUSE_MODE_CONFINED_HIDDEN

func _input(event):
	if Input.is_action_just_pressed(\"quit\"):
		get_tree().quit()

func _process(delta):
	var p := Globals.player
	if p.global_position.y < -20.:
		get_tree().reload_current_scene()
"

